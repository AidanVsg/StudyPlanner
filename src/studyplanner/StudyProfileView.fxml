<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="studyplanner.StudyProfileViewController">
   <children>
      <Label fx:id="profileNameLabel" layoutX="14.0" layoutY="27.0" prefHeight="34.0" prefWidth="253.0" text="\%profile_name%" textFill="#f86300">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Button fx:id="addTaskButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTaskButtonClick" text="Add Task">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="640.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#saveStudyProfile" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <ListView fx:id="assignmentListView" layoutX="171.0" layoutY="136.0" prefHeight="217.0" prefWidth="150.0" />
      <Button layoutX="96.0" layoutY="361.0" mnemonicParsing="false" onAction="#addActivityButtonClick" text="Add Activity">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="193.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTaskButtonClick" text="Add Milestone">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="171.0" layoutY="107.0" prefHeight="17.0" prefWidth="77.0" text="Assignments">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ListView fx:id="taskListView" layoutX="321.0" layoutY="136.0" prefHeight="217.0" prefWidth="150.0" />
      <Label layoutX="321.0" layoutY="107.0" prefHeight="17.0" prefWidth="77.0" text="Tasks">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button layoutX="300.0" layoutY="361.0" mnemonicParsing="false" onAction="#dashboardButtonClick" text="Open Dashboard">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ListView fx:id="moduleListView" layoutX="21.0" layoutY="136.0" prefHeight="217.0" prefWidth="150.0" />
      <Label layoutX="21.0" layoutY="107.0" prefHeight="17.0" prefWidth="77.0" text="Modules">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ListView fx:id="criteriaListView" layoutX="471.0" layoutY="136.0" prefHeight="217.0" prefWidth="150.0" />
      <Label layoutX="471.0" layoutY="107.0" prefHeight="17.0" prefWidth="77.0" text="Criteria">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
