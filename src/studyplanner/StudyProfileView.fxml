<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="studyplanner.StudyProfileViewController">
   <children>
      <Label fx:id="profileNameLabel" layoutX="14.0" layoutY="27.0" prefHeight="34.0" prefWidth="253.0" text="\%profile_name%" textFill="#f86300">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Button fx:id="addTaskButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTaskButtonClick" text="Add Task">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#saveStudyProfile" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <ListView fx:id="assignmentListView" layoutX="19.0" layoutY="114.0" prefHeight="239.0" prefWidth="188.0" />
      <Button fx:id="addTaskButton1" layoutX="102.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTaskButtonClick" text="Add Activity">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="addTaskButton2" layoutX="209.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTaskButtonClick" text="Add Milestone">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="19.0" layoutY="97.0" prefHeight="17.0" prefWidth="77.0" text="Assignments">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="moduleComboBox" layoutX="21.0" layoutY="72.0" prefWidth="150.0" />
      <ListView fx:id="taskListView" layoutX="214.0" layoutY="114.0" prefHeight="239.0" prefWidth="188.0" />
      <Label layoutX="214.0" layoutY="97.0" prefHeight="17.0" prefWidth="77.0" text="Tasks">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
